{
  "name": "anchorpipe",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "nx": "nx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "test": "nx run-many -t test --all",
    "test:coverage": "nx run-many -t test --all --configuration=coverage",
    "test:web": "nx test web",
    "test:watch": "nx test web --configuration=watch",
    "build": "nx run-many -t build --all",
    "affected:lint": "nx affected -t lint",
    "affected:test": "nx affected -t test",
    "affected:build": "nx affected -t build",
    "db:generate": "cd libs/database && npm run db:generate",
    "db:migrate": "cd libs/database && npm run db:migrate",
    "db:migrate:deploy": "cd libs/database && npm run db:migrate:deploy",
    "db:studio": "cd libs/database && npm run db:studio",
    "db:seed": "cd libs/database && npm run db:seed",
    "openapi:validate": "swagger-cli validate apps/web/public/openapi.yaml"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@nx/js": "^22.0.3",
    "@nx/next": "^22.0.3",
    "@nx/node": "^22.0.3",
    "@nx/vite": "22.0.3",
    "@nx/web": "22.0.3",
    "@prisma/client": "^6.19.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/ioredis": "^4.28.10",
    "@types/ioredis-mock": "^8.2.6",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "eslint": "^9.39.1",
    "ioredis-mock": "^8.13.1",
    "jiti": "2.4.2",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "^22.0.2",
    "prettier": "^3.3.2",
    "prisma": "^6.19.0",
    "swagger-cli": "^4.0.4",
    "tslib": "^2.3.0",
    "typescript": "^5.6.2",
    "vite": "^7.0.0",
    "vitest": "^3.0.0"
  },
  "overrides": {
    "styled-jsx": {
      "react": "^19.2.0",
      "react-dom": "^19.2.0"
    },
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0"
  },
  "dependencies": {
    "ioredis": "^5.8.2",
    "prom-client": "^15.1.3"
  },
  "optionalDependencies": {
    "lightningcss-linux-x64-gnu": "^1.30.2",
    "lightningcss-linux-x64-musl": "^1.30.2",
    "lightningcss-win32-x64-msvc": "^1.30.2"
  }
}
