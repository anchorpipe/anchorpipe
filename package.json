{
  "name": "anchorpipe",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "nx": "nx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "test": "nx run-many -t test --all",
    "test:coverage": "cd apps/web && vitest run --coverage --project node && vitest run --project dom",
    "test:web": "nx test web",
    "test:watch": "nx test web --configuration=watch",
    "build": "nx run-many -t build --all",
    "affected:lint": "nx affected -t lint",
    "affected:test": "nx affected -t test",
    "affected:build": "nx affected -t build",
    "db:generate": "cd libs/database && npm run db:generate",
    "db:migrate": "cd libs/database && npm run db:migrate",
    "db:migrate:deploy": "cd libs/database && npm run db:migrate:deploy",
    "db:studio": "cd libs/database && npm run db:studio",
    "db:seed": "cd libs/database && npm run db:seed",
    "openapi:validate": "swagger-cli validate apps/web/public/openapi.yaml",
    "docs:dev": "cd apps/docs && npm run start",
    "docs:build": "cd apps/docs && npm run build",
    "docs:serve": "cd apps/docs && npm run serve",
    "docs:deploy": "cd apps/docs && npm run deploy",
    "lint:docs": "node scripts/validate-docs.mjs lint",
    "spellcheck:docs": "node scripts/validate-docs.mjs spell",
    "validate:docs": "node scripts/validate-docs.mjs all"
  },
  "devDependencies": {
    "@cspell/dict-html": "^4.0.13",
    "@cspell/dict-software-terms": "^5.1.15",
    "@eslint/eslintrc": "^3.3.3",
    "@nx/js": "^22.1.3",
    "@nx/next": "^22.1.3",
    "@nx/node": "^22.1.3",
    "@nx/vite": "22.1.3",
    "@nx/web": "22.1.3",
    "@prisma/client": "^7.1.0",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.15.3",
    "@swc/helpers": "~0.5.11",
    "@types/ioredis": "^5.0.0",
    "@types/ioredis-mock": "^8.2.6",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "cspell": "^9.4.0",
    "eslint": "^9.39.1",
    "ioredis-mock": "^8.13.1",
    "jiti": "2.6.1",
    "jsonc-eslint-parser": "^2.4.2",
    "jsonc-parser": "^3.3.1",
    "markdownlint": "^0.40.0",
    "markdownlint-cli2": "^0.20.0",
    "markdownlint-cli2-formatter-pretty": "^0.0.9",
    "nx": "^22.0.2",
    "prettier": "^3.7.4",
    "prisma": "^7.1.0",
    "swagger-cli": "^4.0.4",
    "tslib": "^2.3.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^3.0.0"
  },
  "overrides": {
    "styled-jsx": {
      "react": "^19.2.0",
      "react-dom": "^19.2.0"
    },
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "hono": "^4.10.3",
    "@hono/node-server": "^1.15.0",
    "tmp": "^0.2.4",
    "@prisma/dev": "0.14.0",
    "valibot": "1.2.0"
  },
  "dependencies": {
    "ioredis": "^5.8.2",
    "prom-client": "^15.1.3"
  },
  "optionalDependencies": {
    "lightningcss-linux-x64-gnu": "^1.30.2",
    "lightningcss-linux-x64-musl": "^1.30.2",
    "lightningcss-win32-x64-msvc": "^1.30.2"
  }
}
