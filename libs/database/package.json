{
  "name": "@anchorpipe/database",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
