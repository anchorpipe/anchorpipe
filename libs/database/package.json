    "db:health": "tsx src/health-check.ts"
  },
  "dependencies": {
    "@prisma/client": "^7.0.1",
    "@prisma/adapter-pg": "^7.0.1",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/pg": "^8.11.10",
    "prisma": "^7.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
